{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "disabled": false,
      "autoApprove": [
        "browser_take_screenshot"
      ]
    },
    "filesystem": {
      "command": "uvx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "/Users/marclosper"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "sqlite": {
      "command": "uvx",
      "args": [
        "mcp-server-sqlite",
        "--",
        "--db-path",
        "/tmp/example.db"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "brave-search": {
      "command": "uvx",
      "args": [
        "mcp-server-brave-search"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "web-search": {
      "command": "uvx",
      "args": [
        "mcp-server-web-search"
      ],
      "disabled": false,
      "autoApprove": []
    }
  }
}